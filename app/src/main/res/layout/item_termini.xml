<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout style="@style/normal_weight_f1f2f4_vertical"
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_height="wrap_content"
                android:background="@color/trans"
    >

    <TextView
        android:id="@+id/muDiDi"
        android:layout_width="170px"
        android:layout_height="wrap_content"
        android:layout_marginBottom="7dp"
        android:gravity="right"
        android:text="用车时间"
        android:textColor="@color/z868788"
        android:textSize="14sp"/>

    <TextView
        android:id="@+id/textMuDiDi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="7dp"
        android:layout_marginLeft="15dp"
        android:layout_toRightOf="@id/muDiDi"
        android:text="现在"
        android:textColor="@color/z262627"
        android:textSize="14sp"/>

    <TextView
        android:id="@+id/textMuDiDiDetail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="7dp"
        android:layout_marginLeft="15dp"
        android:layout_toRightOf="@id/muDiDi"
        android:layout_below="@id/textMuDiDi"
        android:text="现在"
        android:textColor="@color/z262627"
        android:textSize="14sp"/>

</RelativeLayout>

<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="normal_weight">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="normal_weight_vertical" parent="normal_weight">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="normal_weight_f1f2f4_vertical" parent="normal_weight_vertical">
        <item name="android:background">@color/zf1f2f4</item>
    </style>

    <style name="ZddddddLineStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">2px</item>
        <item name="android:background">@color/zdddddd</item>
    </style>

    <style name="ZddddddLineOneStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">2px</item>
        <item name="android:background">@color/zd1d3d4</item>
    </style>

    <style name="ZddddddLineTwoStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">2px</item>
        <item name="android:background">@color/ze6e7e9</item>
    </style>

    <style name="normal_sixteen_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/z535455</item>
        <item name="android:textSize">16sp</item>
    </style>


    <style name="normal_fourteen_edittext">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@color/z262627</item>
        <item name="android:textSize">14sp</item>
        <item name="android:background">@null</item>
        <item name="android:layout_marginLeft">30px</item>
        <item name="android:textColorHint">@color/z868788</item>
        <item name="android:singleLine">true</item>
        <item name="android:textCursorDrawable">@null</item>
    </style>

    <style name="normal_white_sixteen_text" parent="normal_sixteen_text">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="normal_image_next">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@drawable/image_next</item>
    </style>

    <style name="normal_personal_text_edit">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorHint">@color/z868788</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center|left</item>
        <item name="android:layout_marginLeft">125px</item>
        <item name="android:layout_marginRight">30px</item>
    </style>

    <style name="bottom_text" parent="normal_sixteen_text">
        <item name="android:textColor">@color/zadb5bb</item>
        <item name="android:textSize">12sp</item>

    </style>
    <style name="order_title_text" parent="normal_sixteen_text">
        <item name="android:layout_centerInParent">true</item>
        <item name="android:textSize">14sp</item>

    </style>
    <style name="order_title_line" parent="ZddddddLineStyle">
        <item name="android:background">@color/zfb331f</item>
        <item name="android:layout_height">10px</item>
        <item name="android:layout_alignParentBottom">true</item>
    </style>

    <style name="order_title_relative" >
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:clickable">true</item>
    </style>
</resources>
